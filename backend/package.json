{
  "name": "online-booking-system-backend",
  "version": "1.0.0",
  "description": "Backend API for Online Booking System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "debug": "nodemon --inspect server.js",
    "debug-brk": "nodemon --inspect-brk server.js",
    "setup-db": "node database_setup.js",
    "reset-db": "node data_manager.js --action=clear && node data_manager.js --action=setup",
    "generate-data": "node data_manager.js --action=generate --type=all --days=30",
    "db-stats": "node data_manager.js --action=stats",
    "test": "cd ../tests && npm test",
    "test:unit": "cd ../tests && npm run test:unit",
    "test:integration": "cd ../tests && npm run test:integration",
    "lint": "echo \"Linting not configured yet\"",
    "health": "node test-server.js"
  },
  "keywords": [
    "booking",
    "flights",
    "trains",
    "hotels",
    "express",
    "mongodb",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "blob-stream": "^0.1.3",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.1.0",
    "mongoose": "^7.8.7",
    "pdfkit": "^0.17.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
